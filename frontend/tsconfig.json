{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "*.d.ts",
    "src/**/*",
    ".eslintrc.*"
  ],
  "compilerOptions": {
    "lib": [
      "esnext",
      "dom",
      "dom.iterable"
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "types": [
      "vite/client"
    ]
  },
  "references": [
    {
      "path": "tsconfig.node.json"
    }
  ]
}

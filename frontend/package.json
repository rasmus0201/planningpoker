    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/vue-fontawesome": "^3.0.6",
    "@vueuse/components": "^10.9.0",
    "@vueuse/core": "^10.9.0",
    "bulma": "^0.9.4",
    "color": "^4.2.3",
    "laravel-echo": "^1.16.0",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "pusher-js": "^8.3.0",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"

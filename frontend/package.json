    "postcss": "^8.4.35",
    "postcss-html": "^1.6.0",
    "postcss-sass": "^0.5.0",
    "prettier": "^3.2.5",
    "sass": "^1.72.0",
    "typescript": "^5.4.2",
    "vite": "^5.1.6",
    "vite-plugin-mkcert": "^1.17.4",
    "vue-tsc": "^2.0.6"

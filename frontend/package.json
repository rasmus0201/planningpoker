{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "watch": "vite build --watch",
    "production": "run-s lint build-only",
    "build": "run-s type-check build-only",
    "build-only": "vite build",
    "lint": "run-s type-check lint:scripts format-code",
    "lint:scripts": "eslint --config .eslintrc.cjs --fix 'src/**/*.{js,ts,vue,mjs}'",
    "format-code": "prettier --write 'src/**/*.{js,ts,vue,less}'",
